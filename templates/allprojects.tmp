<link rel="stylesheet" href="style/project.css">
<script src="js/project.js"></script>
<div class="projects pure-g"></div>
<script>
    var projectsDiv = document.querySelector('.projects');
    SnapAPI.getPublicProjectList(pageUser())
        .then(function (result) {
            result.PublicProjectList.forEach(function (project) {
                projectsDiv.append(projectDiv(project, { size: 'small', gridSize: 5 }));
            });
        })
        .catch(function (error) {
            genericError(error);
        });
</script>
