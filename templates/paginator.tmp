<nav class="${widgetName} paginator">
    <a href="#" onclick="${widgetName}_toPage(1);"><i class="fas fa-fast-backward"></i></a>
    <a href="#" onclick="${widgetName}_toPage(${widgetName}_page - 5 > 0 ? ${widgetName}_page - 5 : 1);"><i class="fas fa-backward"></i></a>
    <a href="#" onclick="${widgetName}_toPage(${widgetName}_page - 1 > 0 ? ${widgetName}_page - 1 : ${widgetName}_page);"><i class="fas fa-play fa-rotate-180"></i></a>
    <span class="page"></span> / <span class="total-pages"></span>
    <a href="#" onclick="${widgetName}_toPage(${widgetName}_page + 1 < ${widgetName}_totalPages ? ${widgetName}_page + 1 : ${widgetName}_totalPages);"><i class="fas fa-play"></i></a>
    <a href="#" onclick="${widgetName}_toPage(${widgetName}_page + 5 < ${widgetName}_totalPages ? ${widgetName}_page + 5 : ${widgetName}_totalPages);"><i class="fas fa-forward"></i></a>
    <a href="#" onclick="${widgetName}_toPage(${widgetName}_totalPages);"><i class="fas fa-fast-forward"></i></a>
</nav>
