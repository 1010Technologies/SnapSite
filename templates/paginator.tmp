<nav class="${widgetName} paginator">
    <a class="clickable" onclick="${widgetName}_toPage(1);"><i class="fas fa-caret-left"></i></a>
    <a class="clickable" onclick="${widgetName}_toPage(${widgetName}_page - 5 > 0 ? ${widgetName}_page - 5 : 1);"><i class="fas fa-angle-double-left"></i></a>
    <a class="clickable" onclick="${widgetName}_toPage(${widgetName}_page - 1 > 0 ? ${widgetName}_page - 1 : ${widgetName}_page);"><i class="fas fa-angle-left"></i></a>
    <span class="page"></span> / <span class="total-pages"></span>
    <a class="clickable" onclick="${widgetName}_toPage(${widgetName}_page + 1 < ${widgetName}_totalPages ? ${widgetName}_page + 1 : ${widgetName}_totalPages);"><i class="fas fa-angle-right"></i></a>
    <a class="clickable" onclick="${widgetName}_toPage(${widgetName}_page + 5 < ${widgetName}_totalPages ? ${widgetName}_page + 5 : ${widgetName}_totalPages);"><i class="fas fa-angle-double-right"></i></a>
    <a class="clickable" onclick="${widgetName}_toPage(${widgetName}_totalPages);"><i class="fas fa-caret-right"></i></a>
</nav>
